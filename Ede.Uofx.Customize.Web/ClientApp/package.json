    "ng": "ng",
    "start": "run-script-os",
    "start:windows": "ng serve --port 40001 --disable-host-check",
    "start:windows2": "ng serve --port 40001 --disable-host-check --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
