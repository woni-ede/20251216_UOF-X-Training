    "build": "ng build",
    "build:ssr": "ng run uofx-plugin-sample:server:dev",
    "upgrade:uofx": "npm i @uofx/app-components@latest @uofx/app-native@latest @uofx/core@latest @uofx/error-code@latest @uofx/icon@latest @uofx/plugin-api@latest @uofx/web-components@latest",
